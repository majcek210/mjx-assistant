# AI Provider API Keys
# At least one is required for AI operations

# Google Gemini API Key (for Gemini/Gemma models)
# Get yours at: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Groq API Key (for LLaMA/Mixtral models - optional)
# Get yours at: https://console.groq.com/keys
GROQ_API_KEY=your_groq_api_key_here

# Main Agent Configuration
# Which model should act as the main agent (task analyzer/selector)
# Format: origin:model (e.g., google:gemini-1.5-flash or groq:llama-3.3-70b-versatile)
MAIN_AGENT_MODEL=groq:compound

# Discord Bot Token (Required when Discord integration is enabled)
BOT_TOKEN=your_discord_bot_token_here

MASTER_ID = your_discord_id_here
